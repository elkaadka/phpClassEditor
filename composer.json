{
    "name": "kanel/phpclasseditor",
    "description": "Edit php files / classes by dynamically adding code to them",
    "minimum-stability": "stable",
    "require" : {
        "php" : ">=7.0"
    },
    "repositories" : [ {
        "type" : "composer",
        "url" : "https://packagist.org/"
    } ],
    "require-dev" : {
        "phpunit/phpunit": "^6.5"
    },
    "autoload" : {
        "psr-4" : {
            "Kanel\\ClassEditor\\" : "src/",
            "Kanel\\ClassEditor\\Tests\\" : "tests/"
        }
    },
    "authors" : [ {
        "name" : "Adil El Kanabi",
        "email" : "adil.kanabi@gmail.com"
    } ],
    "config": {
        "preferred-install": "dist"
    }
}
